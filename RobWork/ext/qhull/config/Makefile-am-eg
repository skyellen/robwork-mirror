  q_eg          \
  q_egtest      \
  q_test        \
  q_test-ok.txt \
  q_test.bat    \
  Qhull-go.bat  \
  qhulltest-ok.txt

### Extra files to be included in the tarball

EXTRA_DIST = $(examples_DATA)
