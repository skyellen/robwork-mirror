<TaskDescription>
	
	<WorkCell>[workcell filename]</WorkCell>
	<DynamicWorkCell>[dynamic workcell filename]</DynamicWorkCell>
	
	<Interference>
		<Objects>
			<Object>[object 1. name]</Object>
		</Objects>
		<InterferenceLimit>[DOUBLE limit]</Object>
	</Interference>
	
	<Target>[target object name]</Target>
	
	<WrenchLimit>[DOUBLE limit]</WrenchLimit>
	
	<Gripper>
		<Name>[name of the gripper device in the workcell]</Name>
		<TCP>[name of the TCP frame of the gripper]</TCP>
		<Controller>[name of the gripper controller]</Controller>
	</Gripper>
	
</TaskDescription>
