    <RigidBody frame="Peg">
	<Mass>0.3</Mass>
	<Inertia>
	0.00025 0 0
	0 0.00025 0
	0 0 0.000045
	</inertia>
	<COG>
	0 0 0.05
	</COG>
	<Integrator>Euler</Integrator>
	<MaterialID>Plastic</MaterialID>
    </RigidBody>

    <Constraint name="Spring" type="Free" parent="Robot" child="Peg">
	<Transform3D>
	    <Pos>0 0 -0.178</Pos>
            <RPY>3.14159 0 3.14159</RPY>
	</Transform3D>

<Spring>
        <Compliance>
	0.001 0 0 0 0 0
	0 0.001 0 0 0 0
	0 0 0.001 0 0 0
	0 0 0 0.33 0 0
	0 0 0 0 0.33 0
	0 0 0 0 0 0.33
        </Compliance>
        <Damping>
	34 0 0 0 0 0
	0 34 0 0 0 0
	0 0 34 0 0 0
	0 0 0 0.0001 0 0
	0 0 0 0 0.0001 0
	0 0 0 0 0 0.0001
        </Damping>
   	</Spring>
    </Constraint>

 <BodyContactSensor name="NutContactSensor" body="Peg" />

</DynamicWorkcell>
