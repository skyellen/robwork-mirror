	</Frame>
	
	<Drawable name="CellGeo" refframe="CellBaseLoc">
		<RPY> 0 0 0 </RPY> <Pos> 0 0 0</Pos> 
      	<Polytope file="../../objects/robolab_table.obj" />
	</Drawable>

	
	<!-- Robot location -->
	<Frame name="PanaScaraBaseLoc" refframe="CellBaseLoc">
		<RPY>-30 0 0</RPY> <Pos> -0.4 0.4 0.0 </Pos> <!--Rough mesurement-->
	</Frame>
