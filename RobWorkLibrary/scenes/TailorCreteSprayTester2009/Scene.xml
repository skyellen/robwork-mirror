		<RPY> 0 0 0 </RPY> <Pos> 0 0 0 </Pos>
		<Polytope file="geometry/beslag.stl" />
	</Drawable>
		
	<Frame name="SprayTool" refframe="WORLD" type="Movable" daf="true">
		<RPY>90 0 0</RPY> <Pos> 0.03225 0.0425 -0.05 </Pos>
	</Frame>
	<Drawable name="SprayToolGeo" refframe="SprayTool"> 
		<Polytope file="geometry/dyse.stl" />
	</Drawable>
	
	<Frame name="MeasureTool" refframe="WORLD" type="Movable" daf="true">
		<RPY> 90 0 0 </RPY> <Pos> -0.005 0.0425 -0.035 </Pos>
		<Drawable name="Sensor" refframe="MeasureTool"> 
			<RPY> 0 0 0 </RPY><Pos> 0 0 0.035 </Pos>
			<Cylinder radius="0.006" z="0.07" />
		</Drawable>
	</Frame>

	
