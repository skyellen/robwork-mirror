#!/usr/bin/env python

"""
setup.py file for SWIG example
"""

from distutils.core import setup, Extension

#'/D "NDEBUG"','/D "/NODEFAULTLIB:library"
rws_module = Extension('_rws', 
				extra_compile_args=[ @RWS_SWIG_COMPILE_FLAGS@ ],
			   	extra_link_args   =[ @RWS_SWIG_COMPILE_FLAGS@ ],
                sources      = [ @RWS_SWIG_SOURCES@ ],
                include_dirs = [ @RWS_SWIG_DEP_INCLUDE_DIRS@ ],
			    library_dirs = [ @RWS_SWIG_DEP_LIB_DIRS@ ],
			    libraries    = [ @RWS_SWIG_DEP_LIBS@ ]
             )

setup (name = 'rws',
       version = '@ROBWORKSTUDIO_VERSION@',
       author      = "RobWork",
       description = """Simple swig example from docs""",
       ext_modules = [rws_module],
       py_modules = ["rws"],
       )
